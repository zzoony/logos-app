{
  "sentences": [
    {
      "sequence_order": 1,
      "original_text": "We all",
      "korean_translation": "우리 모두는"
    },
    {
      "sequence_order": 2,
      "original_text": "like sheep",
      "korean_translation": "양처럼"
    },
    {
      "sequence_order": 3,
      "original_text": "have gone astray",
      "korean_translation": "길을 잃었으니"
    },
    {
      "sequence_order": 4,
      "original_text": "each of us",
      "korean_translation": "우리 각자가"
    },
    {
      "sequence_order": 5,
      "original_text": "has turned to our own way",
      "korean_translation": "자기 길로 돌아섰고"
    },
    {
      "sequence_order": 6,
      "original_text": "and the Lord",
      "korean_translation": "그리고 주님께서"
    },
    {
      "sequence_order": 7,
      "original_text": "has laid on him",
      "korean_translation": "그에게 담당시키셨으니"
    },
    {
      "sequence_order": 8,
      "original_text": "the iniquity of us all",
      "korean_translation": "우리 모두의 죄악을"
    }
  ]
}